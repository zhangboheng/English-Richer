<swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}" bindchange="swiperChange">
  <block wx:for="{{images}}" wx:key="index">
    <swiper-item>
      <image src="{{item.src}}" class="swiper-image" bindtap="navigateToPage" data-url="{{item.url}}"></image>
    </swiper-item>
  </block>
</swiper>