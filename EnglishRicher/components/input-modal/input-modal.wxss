.modal {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index:9999999999999;
  background: #ffffff88
}

.modal-content {
  width: 60%;
  padding: 20rpx;
  border-radius: 8rpx;
  text-align: center;
  background-color: #f5d659;
  border: 3px solid #000;
}

.input-box {
  height: 40rpx;
  margin-bottom: 20rpx;
  padding: 0 10rpx;
  border: 3px solid #000;
  background-color: #fff;
}

.model-btn {
  background-color: #f5d659;
  border: 3px solid #000;
  width: 100% !important;
}